@model IEnumerable<CRM.Models.ApplicationUser>

<div class="panel panel-default">
    <div class="panel-body status">
        <div class="who clearfix">
            <h4><i class="fa fa-user"></i> เจ้าหน้าที่</h4>
        </div>
        <div class="row">
            <div class="text">

                @{
                    foreach (var Get in Model)
                    {
                        <div class="col-sm-12 col-md-6 col-lg-4">
                            <div class="well text-center connect">
                                <img src="~/uploads/users/@Get.PictureFile" alt="img" class="margin-bottom-5 margin-top-5 img-responsive">
                                <br>
                                <span class="font-xs"><b> <i class="fa fa-user"></i> @Get.FirstName  @Get.LastName</b></span><br>
                                <span class="font-xs"><b> <i class="fa fa-phone"></i> @Get.PhoneNumber </b></span>
                            </div>
                        </div>
                    }
                }
                
            </div>
        </div>
    </div>
</div>